  projectDescription   String?
  budgetAmount         Int
  lastYearSettlement   Int?
  budgetUrl            String?
  description          String?
  from_Proposal_budget Proposal[]  @relation("Proposal_budget")

  @@index([governmentId])
}

model Meeting {
  id                            Int                @id @default(autoincrement())
